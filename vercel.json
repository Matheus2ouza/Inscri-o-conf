{
    "version": 2,
    "builds": [
      {
        "src": "page/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/page/home.html"
      },
      {
        "src": "/inscription",
        "dest": "/page/inscription.html"
      },
      {
        "src": "/payments",
        "dest": "/page/payments.html"
      }
    ]
  }
  
  