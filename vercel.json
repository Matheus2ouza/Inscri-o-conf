{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "page/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "router/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "download/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/page/loginManagement.html"
    },
    {
      "src": "/home",
      "dest": "/page/home.html"
    },
    {
      "src": "/homeAdmin",
      "dest": "/page/homeAdmin.html"
    },
    {
      "src": "/registroUnico",
      "dest": "/page/inscriptionIndiv.html"
    },
    {
      "src": "/registroGrupo",
      "dest": "/page/inscriptionGrup.html"
    },
    {
      "src": "/pagamento",
      "dest": "/page/payment.html"
    },
    {
      "src": "/historico",
      "dest": "/page/history.html"
    },
    {
      "src": "/configuracao",
      "dest": "/page/settings.html"
    },
    {
      "src": "/documentacao",
      "dest": "/page/documentation.html"
    },
    {
      "src": "/localidades",
      "dest": "/page/locality.html"
    },
    {
      "src": "/style/(.*)",
      "dest": "/style/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/download/(.*)",
      "dest": "/downloadimg/$1"
    }
  ]
}
