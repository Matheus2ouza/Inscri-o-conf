{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "page/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "download/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/page/loginManagement.html"
    },
    {
      "src": "/register",
      "dest": "/page/userRegister.html"
    },
    {
      "src": "/verify-email",
      "dest": "/page/verifyEmail.html"
    },
    {
      "src": "/inscricaoServi",
      "dest": "/page/inscriptionServ.html"
    },
    {
      "src": "/inscricaoJonv",
      "dest": "/page/inscriptionError.html"
    },
    {
      "src": "/pagamento",
      "dest": "/page/payment.html"
    },
    {
      "src": "/documentacao",
      "dest": "/page/documentation.html"
    },
    {
      "src": "/dashboard",
      "dest": "/page/dashboard.html"
    },
    {
      "src": "/report",
      "dest": "/page/report.html"
    },
    {
      "src": "/hospedagem",
      "dest": "/page/hostingPage.html"
    },
    {
      "src": "/movimentacao",
      "dest": "/page/movement.html"
    },
    {
      "src": "/comprovante",
      "dest": "/page/proofPayment.html"
    },
    {
      "src": "/vendas",
      "dest": "/page/registro_pagamentos.html"
    },
    {
      "src": "/style/(.*)",
      "dest": "/style/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/download/(.*)",
      "dest": "/downloadimg/$1"
    }
  ]
}
